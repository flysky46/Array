#include <iostream>
#include <vector>
using namespace std;

void printV(vector<int>& nums)
{
    for (int i = 0; i < nums.size(); i++)
        cout<< nums[i]<< " ";
    cout<<endl;
}

int main() 
{
    vector<int> v = {1,2,3,4,5,6,7,8,9};
    //vector<int>::iterator t = v.begin();
    //v.erase(t + 3);
    //v.push_back(10);
    for(vector<int>::iterator iter=v.begin(); iter!=v.end(); )
    {
         if( *iter == 3)
            iter = v.erase(iter);
       else
            iter ++ ;
    }
    printV(v);
    return 0;
}
